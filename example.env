# Copy to .env and adjust values for your setup.
# Local machine checkout in form user@host:/absolute/path
LOCAL_E2E_TARGET=user@localhost:/home/user/code/webdevmcp

# SSH port to reach your local machine from remote dev host
LOCAL_E2E_PORT=8022

# Optional: set to 1 to include headed extension E2E
E2E_RUN_EXTENSION=0

# Optional: branch name to checkout/pull on local machine before running tests
# BRANCH=debug/your-branch

# Optional: if 1, reset and clean local machine repo before branch sync
CLEAN_REMOTE=0
